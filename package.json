{
  "name": "job-diary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix app/",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "migrate:reset": "dotenv -e .env.local prisma migrate reset",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev --name",
    "migrate:generate": "prisma generate",
    "ncu": "ncu",
    "ncu:update": "ncu -u",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.3.1",
    "chakra-dayzed-datepicker": "^0.2.10",
    "dayjs": "^1.11.10",
    "dayzed": "^3.2.3",
    "eslint-config-next": "13.5.2",
    "framer-motion": "^10.16.4",
    "next": "13.5.2",
    "next-auth": "^4.23.1",
    "nodemailer": "^6.9.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.7.0"
  }
}
