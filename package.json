{
  "name": "job-diary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "migrate:reset": "dotenv -e .env.local prisma migrate reset",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev --name",
    "migrate:generate": "prisma generate",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy && next build",
    "ncu": "ncu",
    "ncu:update": "ncu -u"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/next-js": "^2.1.3",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "chakra-dayzed-datepicker": "^0.2.8",
    "dayjs": "^1.11.7",
    "dayzed": "^3.2.3",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "framer-motion": "^10.12.16",
    "next": "13.4.3",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1"
  }
}
